AGENDA
{
    CONTACTOS
    {
        USUARIOS DE LA APP
    }
    MOSTRAR
    {
        INFORMACIÓN DE CONTACTO, COMO CORREO, TELÉFONO, FOTO
    }
}
PERFIL
{
    PRIVADO
    {
        RECIBE SOLICITUD DE AGENDAR
        ELEGIR INDIVIDUALMENTE QUE DATOS QUIERE MOSTRAR
    }
    PUBLICO
    {
        CUALQUIERA LO PUEDE AGENDAR COMO CONTACTO
        ELEGIR INDIVIDUALMENTE QUE DATOS QUIERE MOSTRAR
    }
}
LISTAS
{
   ORDER BY
   {
     PROBAR CON LAS VARIABLES GLOBALES DEL HOSTING
   }
   SECCIONES
   {
      EDITAR,
      ELIMINAR
      CUANDO ESTEN DOBLE CHECK TODAS MARCAR DOBLE CHECK LA LISTA EN CUESTION
   }
  LISTA DE COMPRAS
  {
     COMO EN ALIENS NOTES, POR LISTAS Y GLOBAL
  }
   COMPARTIR 
   {
     DENTRO DE LA APP,
     VIA WHATSAPP,
     VIA CORREO
   }
   DESCARGAR
   {
     FORMATO PDF,
     FORMATO WORD
   }
} 

MANIFIESTOS Y PLANILLAS:
{
    AGREGAR UN GUION AL FINAL DEL RIF O CÉDULA
    COMPARTIR
    {
        DENTRO DE LA APP,
        VIA CORREO,
        VIA WHATSAPP
    }
    ENVIÓ DE MANIFIESTO AUTOMÁTICO
    RECORDARLO DE AGREGAR LA FECHA DE ZARPE SI NO SE HA PUESTO
}
RESPONSABLES
{
    AGREGAR UN GUION EN EL RIF O CÉDULA
}

NOTAS
{
    COMPARTIR
    {
        DENTRO DE LA APP,
        VIA WHATSAPP
    }
    OPCIÓN DE CONVERTIR EN LISTA
}

TAREAS
{
    LISTA DE TAREAS
    {
        UN TITULO PARA AGREGARLE TAREAS ADENTRO
    }
    COMPARTIR
    {
        DENTRO DE LA APP,
        VIA WHATSAPP
    }
    RECORDATORIO
    OPCIÓN DE CONVERTIR EN LISTA
}
EXTRA
{
    AGRUPAR NOTAS, LISTAS, TAREAS POR FECHAS
    REVISAR POR QUE EL INDEX-FOOTER SE SUBE CON EL TECLADO EN MOVIL
}

SIGUE ENFOCADO, SI ALGUIEN PUEDE, ESE ERES TU.